{
  "schemaVersion": 1,
  "id": "obscraft",
  "version": "${version}",
  "name": "OBSCraft",
  "description": "Use OBS WebSocket Server to control OBS from Minecraft.",
  "authors": [
    "MoSadie"
  ],
  "contact": {
    "homepage": "https://github.com/MoSadie/OBSCraft",
    "sources": "https://github.com/MoSadie/OBSCraft",
    "issues": "https://github.com/MoSadie/OBSCraft/issues"
  },
  "license": "GNU LGPL v3",
  "icon": "assets/obscraft/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.mosadie.obscraft.fabric.client.ObsCraftFabricClient"
    ]
  },
  "mixins": [
    "obscraft.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.3",
    "minecraft": "~1.21",
    "java": ">=21",
    "architectury": ">=13.0.2",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
